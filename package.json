  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josmardias/bike-vitoria.git"
  },
  "main": "index.js",
  "scripts": {
    "fixpack": "fixpack",
    "lint": "eslint .",
    "lint:fix": "npm run lint --silent -- --fix",
    "preinstall": "command -v nvc >/dev/null 2>&1; if [ $? -eq 0 ]; then nvc; fi",
    "prepublish": "publish-please guard",
    "pretest": "nvc",
    "publish-please": "publish-please",
    "start": "node $npm_package_main",
    "test": "mocha --opts scripts/mocha.opts src/**/*-test.js",
    "test:debug": "node-debug _mocha --opts scripts/mocha.opts src/**/*-test.js"
  },
  "dependencies": {
    "babel-preset-es2015-node": "~6.1.0",
    "babel-register": "~6.9.0",
    "chalk": "~1.1.3",
    "columnify": "~1.5.4",
    "commander": "~2.9.0",
    "lodash": "~4.13.1",
    "ora": "~0.2.3",
    "request": "~2.73.0",
    "request-promise": "~4.0.0"
  },
  "devDependencies": {
    "babel-core": "~6.10.4",
    "babel-eslint": "~6.1.0",
    "babel-polyfill": "~6.9.1",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "eslint": "~3.1.0",
    "eslint-config-airbnb-base": "~4.0.0",
    "eslint-plugin-babel": "~3.3.0",
    "eslint-plugin-import": "~1.11.0",
    "eslint-plugin-mocha": "~4.0.0",
    "fixpack": "~2.3.1",
    "get-stream": "~2.3.0",
    "mocha": "~3.0.2",
    "mocha-clean": "~1.0.0",
    "nock": "~8.0.0",
    "node-inspector": "~0.12.8",
    "node-version-checker": "~2.0.0",
    "publish-please": "~2.1.4"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
