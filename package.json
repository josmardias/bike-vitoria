{
  "name": "@josmardias/bike-vitoria",
  "version": "0.0.1",
  "description": "Fetch Bike Vitoria status",
  "license": "MIT",
  "author": "Josmar Dias",
  "homepage": "https://github.com/josmardias/bike-vitoria#readme",
  "bugs": {
